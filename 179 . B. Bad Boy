{
    for(ll i=0; i<10; i++) {};
}
int main()
{

    fastread();




    ll t;

    cin>>t;


    while(t--)
    {
        ll n,m;
        cin>>n>>m;

        ll x,y;cin>>x>>y;


    ll a,b;

    a=1;
    b=1;

    if(a==x and b==y)cout<<1<<" "<<min(2ll,y)<<" ";
    else cout<<a<<" "<<b<<" ";


  a=n;
  b=m;

  if(a==x and y==m)cout<<n<<" "<<max(1ll,m-1)<<endl;
  else cout<<n<<" "<<m<<endl;




    }



}
