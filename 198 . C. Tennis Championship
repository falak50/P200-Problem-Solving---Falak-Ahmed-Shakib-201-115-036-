int main()
{

      fastread();

   ll a=1;
   ll b=1;

   ll sum=a+b;

   ll cnt=0;

   ll n;

   cin>>n;

   while(sum<=n)
   {

      a=b;
      b=sum;
      sum=a+b;
       cnt++;

      // cout<<sum<<endl;
   }



   cout<<cnt<<endl;




}
