const ll N=2005,mod=1000000007;
int main()
{
  fastread();


  ll t;

  cin>>t;


  while(t--)
  {


   ll n;
   cin>>n;

  n=n*2;

  ll z=0,o=0;

  for(ll i=0;i<n;i++)
  {
      ll a;

      cin>>a;
      if(a%2)o++;
      else z++;
  }


   if(o==z)cout<<"Yes"<<endl;
   else cout<<"No"<<endl;

  }





}


