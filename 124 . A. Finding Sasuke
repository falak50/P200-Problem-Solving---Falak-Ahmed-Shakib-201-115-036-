#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define fastread() (ios_base:: sync_with_stdio(false),cin.tie(NULL))
typedef long long ll;
typedef unsigned long long ull;
typedef pair<ll,ll>pll;
typedef pair<pair<ll,ll>,ll>plll;
#define ff first
#define ss second
#define fi first
#define se second
#define pb push_back
#define eb emplace_back
int main()
{
    fastread();

   ll t;

   cin>>t;

   while(t--)
   {
       ll n;

       cin>>n;
       vector<ll>v;
       ll ara[n+1];
       for(ll i=0;i<n;i++)
       {
           cin>>ara[i];
       }

       for(ll i=0;i<n;i+=2)
       {
           v.pb(-ara[i+1]);
           v.pb(ara[i]);
       }

       for(auto x:v)cout<<x<<" ";cout<<endl;


   }



}
