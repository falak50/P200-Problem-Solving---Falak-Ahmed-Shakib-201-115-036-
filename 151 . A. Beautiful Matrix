#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define fastread() (ios_base:: sync_with_stdio(false),cin.tie(NULL))
typedef long long ll;
typedef unsigned long long ull;
typedef pair<ll,ll>pll;
typedef pair<pair<ll,ll>,ll>plll;
#define ff first
#define ss second
#define fi first
#define se second
#define pb push_back
#define eb emplace_back
#define all(x) x.begin(),x.end()
void loop()
{
    for(ll i=0; i<10; i++) {};
}
const ll N=1e7+10;

int main()
{

  fastread();


  ll n=5;

  ll x,y;

  for(ll i=1;i<=n;i++)
  {
      for(ll j=1;j<=n;j++)
      {
          ll a;
          cin>>a;

          if(a)
          {
              x=i;
              y=j;
          }
      }
  }


  cout<<abs(x-3)+abs(y-3)<<endl;



}


