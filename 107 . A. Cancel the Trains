#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define fastread() (ios_base:: sync_with_stdio(false),cin.tie(NULL))
typedef long long ll;
typedef unsigned long long ull;
typedef pair<ll,ll>pll;
typedef pair<pair<ll,ll>,ll>plll;
#define ff first
#define ss second
#define fi first
#define se second
#define pb push_back
#define eb emplace_back
#define all(x) x.begin(),x.end()
void loop()
{
    for(ll i=0;i<10;i++){};
}
int main()
{
    fastread();

ll t;

cin>>t;


while(t--)
{

    ll n,m;

    cin>>n>>m;

    map<ll,ll>mp;

    for(ll i=0;i<n;i++)
    {
        ll a;
     cin>>a;
       mp[a]=1;
    }
   ll cnt=0;
    for(ll i=0;i<m;i++)
    {
        ll a;
        cin>>a;

        if(mp[a])cnt++;
    }


    cout<<cnt<<endl;


}




}
