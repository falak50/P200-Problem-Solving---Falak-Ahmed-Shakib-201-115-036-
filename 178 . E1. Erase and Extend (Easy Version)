#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define fastread() (ios_base:: sync_with_stdio(false),cin.tie(NULL))
typedef long long ll;
typedef unsigned long long ull;
typedef pair<ll,ll>pll;
typedef pair<pair<ll,ll>,ll>plll;
#define ff first
#define ss second
#define fi first
#define se second
#define pb push_back
#define eb emplace_back
#define all(x) x.begin(),x.end()
void loop()
{
    for(ll i=0; i<10; i++) {};
}
int main()
{

    fastread();



    ll n,k;

    cin>>n>>k;

    string str;

    cin>>str;

    vector<string>v;


    string ans;


  string s;
    for(ll i=0;i<n;i++)
    {
     s+=str[i];

   string ans;
     for(ll i=0;i<k;i++)
     {
        ans+=s;

        if(ans.size()>k)break;
     }

     string ss;
     for(ll i=0;i<k;i++)ss+=ans[i];

     v.pb(ss);

    }

    sort(all(v));

    cout<<v[0]<<endl;

}
