#include<bits/stdc++.h>
using namespace std;
using ll = long long;
#define pb push_back
#define ff first
#define ss second
#define pb push_back
#define eb emplace_back
typedef pair<ll,ll>pll;
typedef pair<ll,pair<ll,ll>>plll;
int main()
{
 
    ll n;
 
    cin>>n;
   if(n==1)cout<<1<<endl;
   else if(n<4)cout<<"NO SOLUTION"<<endl;
    else {
 
        for(ll i=2;i<=n;i+=2)cout<<i<<" ";
 
        for(ll i=1;i<=n;i+=2)cout<<i<<" ";
 
        cout<<endl;
 
 
    }
 
 
}
