#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define fastread() (ios_base:: sync_with_stdio(false),cin.tie(NULL))
typedef long long ll;
typedef unsigned long long ull;
typedef pair<ll,ll>pll;
typedef pair<pair<ll,ll>,ll>plll;
#define ff first
#define ss second
#define fi first
#define se second
#define pb push_back
#define eb emplace_back

int main()
{
    fastread();

     ll n,k;

     cin>>n>>k;
     string s;



     while(1)
     {


     for(char i='a';i<k+'a';i++)
{
        s+=i;

        if(s.size()==n)
        {
            cout<<s<<endl;
            return 0;
        }
        for(char j=i+1;j<k+'a';j++)
        {
            s+=i;
              if(s.size()==n)
        {
            cout<<s<<endl;
            return 0;
        }
        s+=j;
         if(s.size()==n)
        {
            cout<<s<<endl;
            return 0;
        }


        }
}

     }

}
