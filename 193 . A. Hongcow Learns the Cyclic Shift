int main()
{

    fastread();

  string  str;

  cin>>str;


  set<string>st;

   st.insert(str);

 ll t=str.size()+3;

 ll f=0,s=0;
 while(f<str.size())
{
    string ss;
       for(ll i=s;str[i];i++)
     {
          ss+=str[i];
     }
     for(ll i=0;i<f;i++)
     {
         ss+=str[i];
     }
  //   cout<<ss<<endl;
