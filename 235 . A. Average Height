#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define fastread() (ios_base:: sync_with_stdio(false),cin.tie(NULL))
typedef long long ll;
typedef unsigned long long ull;
typedef pair<ll,ll>pll;
typedef pair<pair<ll,ll>,ll>plll;
#define ff first
#define ss second
#define pb push_back
#define eb emplace_back
int main()
{
    fastread();


    ll t;

    cin>>t;

    while(t--)
    {

    ll n;

    cin>>n;


     vector<ll>a,b;


     for(ll i=0;i<n;i++)
     {
         ll c;

         cin>>c;

         if(c%2)a.pb(c);
         else b.pb(c);
     }


     for(auto x:a)cout<<x<<" ";

     for(auto x:b)cout<<x<<" ";

     cout<<endl;




    }


}
